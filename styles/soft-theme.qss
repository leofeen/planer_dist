    image: url(./img/soft-theme/check-box-hover.svg);
}
#settingCheckBox::indicator:checked:hover{
    image: url(./img/soft-theme/check-box-indicator-hover.svg);
}
#settingCheckBox::indicator:checked{
    image: url(./img/soft-theme/check-box-indicator.svg);
}
#settingCheckBox::indicator:unchecked:disabled{
    image: url(./img/soft-theme/check-box-disabled.svg);
    }
#settingCheckBox::indicator:checked:disabled{
    image: url(./img/soft-theme/check-box-indicator-disabled.svg);
