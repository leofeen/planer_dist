    border-radius: 10px;
}
#timestampButton:hover{
    border-color: #9fd1ff;
}
#timestampButton:pressed{
    border-color: #82c3ff;
}

#settingCheckBox::indicator:unchecked{
    image: url(./img/soft-theme/check-box.svg);
}
